  <body>
    <outline text="Legend">
      <outline text="Test is written"/>
      <outline text="Test is not written, but supported by tfm and esrepro"/>
      <outline text="Test is not written, and is not supported by tfm/repro"/>
      <outline text="Test is written, but does not work due to lack of support"/>
    </outline>
    <outline text="Registrar functionality">
      <outline text="Authentication off">
        <outline text="new registration scenarios" description="A UAC attempts to register new contacts">
          <outline text="Sunny day cases">
            <outline text="Basic register" description="A UAC attempts to register a single new contact"/>
            <outline text="Multi register" description="A UAC attempts to register multiple new contacts with one request"/>
            <outline text="Multi register, different expirations" description="A UAC attempts to register multiple new contacts with one request, with different expiries for each contact"/>
            <outline text="Third party registration" description="A UAC attempts to register a contact that does not point to itself"/>
          </outline>
          <outline text="Rainy Day cases">
            <outline text="200 OK dropped" description="The first REGISTER/200 does not make it to the UAC"/>
            <outline text="No user in contact" description="The supplied contact has no user"/>
            <outline text="No user in aor" description="The supplied aor (the Request-Uri) has no user"/>
            <outline text="Contact and aor identical" description="Contact and aor are the same uri, leading to a loop"/>
            <outline text="Bad characters in contact" description="The contact contains characters that are not allowed in a uri"/>
            <outline text="Bad characters in aor" description="The aor contains characters that are not allowed in a uri"/>
            <outline text="Aor does not exist" description="The registrar is not responsible for the aor in the Request-Uri"/>
            <outline text="Expiry too large" description="The register request contains an expiry value larger than 2^32 - 1"/>
            <outline text="Oversize call id" description="The call id in the REGISTER is unreasonably large"/>
            <outline text="Oversize contact" description="The contact is unreasonably large"/>
          </outline>
        </outline>
        <outline text="refresh type scenarios" description="A UAC attempts to refresh contacts">
          <outline text="Sunny day cases">
            <outline text="Basic registration update" description="A UAC re-registers an existing contact"/>
            <outline text="Multi-registration update" description="A UAC re-registers a set of existing contacts"/>
            <outline text="Third party" description="Third party refresh"/>
          </outline>
          <outline text="Rainy day cases">
            <outline text="Expiry too large"/>
            <outline text="Out of sequence call id" description="The call ID for this REGISTER request is out of sequence when compared to the request that established the contact being refreshed"/>
          </outline>
        </outline>
        <outline text="unregister type scenarios" description="A UAC attempts to unregister contacts">
          <outline text="Sunny day">
            <outline text="Basic unregister" description="A UAC attempts to unregister an existing contact"/>
            <outline text="Multi unregister" description="A UAC attempts to unregister multiple existing contacts"/>
            <outline text="Redundant unregister" description="A UAC attempts to unregister a non-existent contact"/>
            <outline text="Unregister *" description="A UAC attempts to use * as the contact in an unregister request"/>
          </outline>
          <outline text="Rainy day">
            <outline text="Unregister * with a non-zero Expires header"/>
            <outline text="aor doesn't exist"/>
          </outline>
        </outline>
        <outline text="fetch type scenarios" description="A UAC attempts to fetch contacts">
          <outline text="Sunny day">
            <outline text="Basic registration fetch" description="A UAC attempts to fetch the bindings of a given aor"/>
          </outline>
          <outline text="Rainy day">
            <outline text="Aor doesn't exist"/>
            <outline text="bad aor"/>
          </outline>
        </outline>
      </outline>
      <outline text="Authentication on">
        <outline text="new registration scenarios" description="A UAC attempts to register new contacts">
          <outline text="Sunny day cases">
            <outline text="Basic register" description="A UAC attempts to register a single new contact" Status="Passing">
              <outline text="testRegisterBasic in sanityTests.cxx"/>
              <outline text="testRegisterBasic in estacadoTests.cxx"/>
            </outline>
            <outline text="Multi register" description="A UAC attempts to register multiple new contacts with one request" Status="Passing">
              <outline text="testMultiple1 in sanityTests.cxx"/>
              <outline text="testMultiple1 in estacadoTests.cxx"/>
            </outline>
            <outline text="Multi register, different expirations" description="A UAC attempts to register multiple new contacts with one request, with different expiries for each contact" Status="Passing">
              <outline text="testMixedExpires in sanityTests.cxx"/>
              <outline text="testMixedExpires in estacadoTests.cxx"/>
            </outline>
            <outline text="Third party registration" description="A UAC attempts to register a contact that does not point to itself" Status="Passing">
              <outline text="testThirdPartyRegistration in estacadoTests.cxx"/>
            </outline>
            <outline text="Test whether registrar is paying attention to q-value" Status="Failing (esrepro doesn&apos;t handle q-value yet)">
              <outline text="testDetailsQValue in sanityTests.cxx"/>
              <outline text="testDetailsQValue in estacadoTests.cxx"/>
              <outline text="q-value is optional, and not yet supported in esrepro"/>
            </outline>
            <outline text="Test if registrar is paying attention to expires parameter" Status="Passing">
              <outline text="testDetailsExpires in estacadoTests.cxx"/>
              <outline text="testDetailsExpires in sanityTests.cxx"/>
            </outline>
          </outline>
          <outline text="Rainy Day cases">
            <outline text="REGISTER/407 dropped" description="The auth challenge is dropped" Status="Passing">
              <outline text="testRegister407Dropped in estacadoTests.cxx"/>
            </outline>
            <outline text="bogus credentials" description="the UAC supplies bad credentials" Status="Passing">
              <outline text="testRegisterBogusAuth in estacadoTests.cxx"/>
            </outline>
            <outline text="Late digest response" description="The UAC waits too long to respond to the challenge (the nonce expires)" Status="Not used">
              <outline text="testRegisterLateDigestResponse in estacadoTests.cxx"/>
            </outline>
            <outline text="200 OK dropped" description="The first REGISTER/200 does not make it to the UAC" Status="Passing">
              <outline text="testRegisterClientRetransmits in sanityTests.cxx ~172"/>
              <outline text="testRegisterClientRetransmits in estacadoTests.cxx"/>
            </outline>
            <outline text="No user in aor (in To: header)" description="The supplied aor (the Request-Uri) has no user" Status="Failing">
              <outline text="testRegisterNoUserInTo in estacadoTests.cxx"/>
            </outline>
            <outline text="User present in Request-Uri" description="This is forbidden in RFC 3261, page 56" Status="Failing">
              <outline text="testRegisterUserInReqUri in estacadoTests.cxx"/>
            </outline>
            <outline text="Bad characters in contact" description="The contact contains characters that are not allowed in a uri">
              <outline text="Cannot construct garbage uris. Would need direct access to the message buffer."/>
            </outline>
            <outline text="Bad characters in aor" description="The aor contains characters that are not allowed in a uri">
              <outline text="Cannot construct a garbage uri, would need direct access to the message's buffer."/>
            </outline>
            <outline text="Not responsible for host in AOR" description="The registrar is not responsible for the aor in the To header" Status="Failing">
              <outline text="testRegisterUnknownAorHost in estacadoTests.cxx"/>
            </outline>
            <outline text="No such user" description="User part in AOR is unknown" Status="Failing">
              <outline text="testRegisterUnknownAorUser in estacadoTests.cxx"/>
            </outline>
            <outline text="Expiry too large" description="The register request contains an expiry value larger than 2^32 - 1">
              <outline text="Would need direct access to message buffer for this."/>
            </outline>
            <outline text="Expiry malformed">
              <outline text="Probably would require access to the message buffer. (Malformed expiry SHOULD be taken as 3600 as per 3261"/>
            </outline>
            <outline text="Oversize call id" description="The call id in the REGISTER is unreasonably large" Status="Failing (Sip stack drops the message it seems, check this out in ethereal)">
              <outline text="testOversizeCallIdRegister in sanityTests.cxx"/>
              <outline text="testOversizeCallIdRegister in estacadoTests.cxx"/>
            </outline>
            <outline text="Oversize contact" description="The contact is unreasonably large" Status="Failing (SipStack drops the message it seems, check this out in ethereal)">
              <outline text="testOversizeContactRegister in sanityTests.cxx"/>
              <outline text="testOversizeContactRegister in estacadoTests.cxx"/>
            </outline>
          </outline>
        </outline>
        <outline text="refresh type scenarios" description="A UAC attempts to refresh contacts">
          <outline text="Sunny day cases">
            <outline text="Basic registration update" description="A UAC re-registers an existing contact" Status="Passing">
              <outline text="testRefresh in sanityTests.cxx"/>
              <outline text="testRefresh in estacadoTests.cxx"/>
            </outline>
            <outline text="Multi-registration update" description="A UAC re-registers a set of existing contacts" Status="Passing">
              <outline text="testRefreshMulti in estacadoTests.cxx"/>
            </outline>
            <outline text="Third party" description="Third party refresh" Status="Passing">
              <outline text="testRefreshThirdParty in estacadoTests.cxx"/>
            </outline>
            <outline text="q-value update" Status="Failing (esrepro doesn&apos;t handle q-value yet)">
              <outline text="testChangeQValue in sanityTests.cxx"/>
              <outline text="testChangeQValue in estacadoTests.cxx"/>
            </outline>
          </outline>
          <outline text="Rainy day cases">
            <outline text="Expiry too large"/>
            <outline text="Out of sequence call id" description="The call ID for this REGISTER request is out of sequence when compared to the request that established the contact being refreshed" Status="Failing (esrepro apparently does not enforce call-id sequence)">
              <outline text="testRegisterOutOfSequence in sanityTests.cxx"/>
              <outline text="testRegisterOutOfSequence in estacadoTests.cxx"/>
            </outline>
          </outline>
        </outline>
        <outline text="unregister type scenarios" description="A UAC attempts to unregister contacts">
          <outline text="Sunny day">
            <outline text="Basic unregister" description="A UAC attempts to unregister an existing contact" Status="Passing">
              <outline text="testSetThenRemoveSpecific in sanityTests.cxx"/>
              <outline text="testSetThenRemoveSpecific in estacadoTests.cxx"/>
            </outline>
            <outline text="Multi unregister" description="A UAC attempts to unregister multiple existing contacts" Status="Passing">
              <outline text="testUnregisterMulti in estacadoTests.cxx"/>
            </outline>
            <outline text="Redundant unregister" description="A UAC attempts to unregister a non-existent contact" Status="Passing">
              <outline text="testUnregisterExpired in sanityTests.cxx"/>
              <outline text="testUnregisterExpired in estacadoTests.cxx"/>
            </outline>
            <outline text="Unregister *" description="A UAC attempts to use * as the contact in an unregister request" Status="Passing">
              <outline text="testUnregisterAll in sanityTests.cxx"/>
              <outline text="testUnregisterAll in estacadoTests.cxx"/>
            </outline>
          </outline>
          <outline text="Rainy day">
            <outline text="Unregister * with a non-zero Expires header" Status="Passing">
              <outline text="testUnregisterAllBadExpires in estacadoTests.cxx"/>
            </outline>
            <outline text="aor doesn't exist" Status="Failing">
              <outline text="testUnregisterNonExistent in estacadoTests.cxx"/>
            </outline>
          </outline>
        </outline>
        <outline text="fetch type scenarios" description="A UAC attempts to fetch contacts">
          <outline text="Sunny day">
            <outline text="Basic registration fetch" description="A UAC attempts to fetch the bindings of a given aor" Status="Passing">
              <outline text="testFetch in sanityTests.cxx"/>
              <outline text="testFetch in estacadoTests.cxx"/>
            </outline>
            <outline text="Check whether registrations are expiring properly" Status="Passing">
              <outline text="testExpiryCleanup in sanityTests.cxx"/>
              <outline text="testExpiryCleanup in estacadoTests.cxx"/>
            </outline>
          </outline>
          <outline text="Rainy day">
            <outline text="Aor doesn't exist" Status="Failing">
              <outline text="testFetchNonExistent in estacadoTests.cxx"/>
            </outline>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="Proxy functionality: INVITE transactions">
      <outline text="no auth">
        <outline text="Non-forking INVITE scenarios" description="INVITE transaction with no forking">
          <outline text="Sunny day">
            <outline text="Basic INVITE" description="basic INVITE transaction, (no CANCEL, BYE, etc)"/>
            <outline text="Basic INVITE with caller BYE" description="basic INVITE transaction, followed by the caller hanging up"/>
            <outline text="Basic INVITE with callee BYE" description="basic INVITE, where callee hangs up"/>
            <outline text="Basic INVITE with CANCEL" description="basic INVITE, where caller CANCELs"/>
          </outline>
          <outline text="Cloudy day">
            <outline text="spiral test for basic INVITE"/>
            <outline text="spiral test with CANCEL"/>
            <outline text="No contacts found"/>
            <outline text="UAS responds with 4xx class response"/>
            <outline text="UAS responds with 5xx class response"/>
            <outline text="UAS responds with 6xx class response"/>
            <outline text="UAS spams 180s"/>
          </outline>
          <outline text="Rainy day">
            <outline text="UAS does not respond (timer C test)"/>
            <outline text="UAS spams 200s"/>
            <outline text="UAS sends 200, then 180"/>
            <outline text="UAC spams INVITEs"/>
            <outline text="UAC spams ACKs"/>
            <outline text="Contact is self"/>
            <outline text="4xx response dropped on server-side"/>
            <outline text="4xx response dropped on client side"/>
            <outline text="5xx response dropped on server side"/>
            <outline text="5xx response dropped on client side"/>
            <outline text="6xx response dropped on server side"/>
            <outline text="6xx response dropped on client side"/>
            <outline text="No route to contact (DNS failure)"/>
            <outline text="Transport down at contact"/>
