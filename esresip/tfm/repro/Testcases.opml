        </outline>
        <outline text="Forking INVITE scenarios" description="INVITE transaction with forking">
          <outline text="Sunny day">
            <outline text="Basic forking INVITE, one 200 response">
              <outline text="testInviteForkOneAnswers in sanityTests.cxx"/>
            </outline>
            <outline text="Forking INVITE, one 200 and rest busy (486)">
              <outline text="testInviteForkOneBusy in sanityTests.cxx"/>
            </outline>
            <outline text="Forking INVITE, all busy">
              <outline text="testInviteAllBusyContacts and testInviteForkBothBusy in sanityTests.cxx"/>
            </outline>
            <outline text="Forking INVITE, with CANCEL">
              <outline text="testInviteForkThreeCallerCancels and testInviteForkCallerCancels in sanityTests.cxx"/>
            </outline>
            <outline text="Forking INVITE, caller sends  BYE"/>
            <outline text="Forking INVITE, callee sends BYE"/>
          </outline>
          <outline text="Cloudy day">
            <outline text="Forking invite with a spiral (spiral is after fork)"/>
            <outline text="4xx class responses (other than 486)"/>
            <outline text="One 4xx class response, one 200"/>
            <outline text="5xx class responses"/>
            <outline text="One 5xx, one 200"/>
            <outline text="6xx class responses"/>
            <outline text="One 6xx, one 200" description="What is supposed to happen here?"/>
            <outline text="5xx and 4xx mixed"/>
            <outline text="6xx and 5xx mixed"/>
            <outline text="6xx and 4xx mixed"/>
            <outline text="200, 4xx, 5xx, and 6xx mixed"/>
            <outline text="Fork merges (ie, both ends of fork are directed at the same UAC)"/>
            <outline text="All contacts answer"/>
            <outline text="All contacts answer, without ringing">
              <outline text="testInviteForBothAnswerNoProvisional in sanityTests.cxx"/>
            </outline>
          </outline>
          <outline text="Rainy day">
            <outline text="INVITE/200 dropped on client side">
              <outline text="testForkedInviteClientLateAck in sanityTests.cxx"/>
            </outline>
            <outline text="Loop on one leg of fork"/>
            <outline text="All contacts time out (Timer C)">
              <outline text="testTimerCForked in sanityTests.cxx"/>
            </outline>
            <outline text="Forking spiral" description="Can tfm handle this?"/>
          </outline>
        </outline>
      </outline>
      <outline text="with auth">
        <outline text="Non-forking INVITE scenarios" description="INVITE transaction with no forking">
          <outline text="Sunny day">
            <outline text="Basic INVITE" description="basic INVITE transaction, (no CANCEL, BYE, etc)" Status="Passing">
              <outline text="testInviteBasic and testDigestInviteBasic in sanityTests.cxx"/>
              <outline text="testInviteBasic in estacadoTests.cxx"/>
            </outline>
            <outline text="Basic INVITE with caller BYE" description="basic INVITE transaction, followed by the caller hanging up" Status="Failing (BYE gets challenged)">
              <outline text="testInviteCallerHangsUp in sanityTests.cxx"/>
              <outline text="testInviteCallerHangsUp in estacadoTests.cxx"/>
            </outline>
            <outline text="Basic INVITE with callee BYE" description="basic INVITE, where callee hangs up" Status="Failing (BYE gets challenged)">
