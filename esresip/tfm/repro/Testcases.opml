              <outline text="testInvite503BeatsCancelClientSide in estacadoTests.cxx"/>
            </outline>
            <outline text="No contacts found" Status="Passing">
              <outline text="testInviteNotFound in sanityTests.cxx"/>
              <outline text="testInviteNotFound in estacadoTests.cxx"/>
            </outline>
            <outline text="UAS responds with 4xx class response">
              <outline text="testInvite488Response in estacadoTests.cxx"/>
              <outline text="testInvite480Response in estacadoTests.cxx" Status="Passing"/>
            </outline>
            <outline text="UAS responds with 5xx class response">
              <outline text="testInvite500Response in estacadoTests.cxx "/>
              <outline text="testInvite503Response in estacadoTests.cxx " Status="Passing"/>
            </outline>
            <outline text="UAS responds with 6xx class response">
              <outline text="testInvite600Response in estacadoTests.cxx "/>
              <outline text="testInvite603Response in estacadoTests.cxx "/>
            </outline>
            <outline text="UAS spams 180s" Status="Passing">
              <outline text="testInviteServerSpams180 in estacadoTests.cxx"/>
            </outline>
            <outline text="bad authentication" Status="Passing">
              <outline text="testInviteBogusAuth in estacadoTests.cxx"/>
            </outline>
          </outline>
          <outline text="Rainy day">
            <outline text="407 is dropped (retransmit of INVITE)" Status="Passing">
              <outline text="testInvite407Dropped in estacadoTests.cxx"/>
            </outline>
            <outline text="ACK/407 is dropped (retransmit of 407)" Status="Failing (tfm is ACKing the wrong thing when the 407 is retransmitted)">
              <outline text="testInviteAck407Dropped in estacadoTests.cxx"/>
