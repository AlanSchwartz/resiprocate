            <outline text="spiral test for basic INVITE" Status="Passing">
              <outline text="testSpiral in sanityTests.cxx"/>
              <outline text="testSpiral in estacadoTests.cxx"/>
            </outline>
            <outline text="spiral test with CANCEL" Status="Passing">
              <outline text="testSpiralWithCancel in estacadoTests.cxx"/>
